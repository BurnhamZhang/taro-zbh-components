    "@tarojs/cli": "^2.1.5",
    "@tarojs/components": "2.1.5",
    "@tarojs/mini-runner": "2.1.5",
    "@tarojs/plugin-babel": "2.1.5",
    "@tarojs/plugin-csso": "2.1.5",
    "@tarojs/plugin-sass": "2.1.5",
    "@tarojs/plugin-uglifyjs": "2.1.5",
    "@tarojs/router": "2.1.5",
    "@tarojs/taro": "2.1.5",
    "@tarojs/taro-weapp": "2.1.5",
    "@tarojs/webpack-runner": "2.1.5",
    "@types/react": "^16.4.6",
    "@types/sinon": "^5.0.5",
    "@types/webpack-env": "^1.13.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
